    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "args" :[
                "-socket=/tmp/nnf.sock",
            ]
        },
        {
            "name": "Test Current File (Nate)",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${relativeFileDirname}",
            "args": [
                "-ginkgo.v",
                "-ginkgo.progress",
            ],
            "env": {
                "KUBEBUILDER_ASSETS": "/Users/roiger/Library/Application Support/io.kubebuilder.envtest/k8s/1.22.1-darwin-amd64"
            },
            "showLog": true
        }
    ]
}