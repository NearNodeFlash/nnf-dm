    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Current File (Nate)",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${relativeFileDirname}",
            "args": [
                "-ginkgo.v",
                "-ginkgo.progress",
            ],
            "env": {
                "KUBEBUILDER_ASSETS": "${workspaceFolder}/bin"
            },
            "showLog": true
        }
    ]
}